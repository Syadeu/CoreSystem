<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="Styles/color-palatte.uss" />
    <Style src="Styles/inventory-style.uss" />
    <ui:VisualElement name="InventoryView" class="default-border" style="height: 65%; width: 35%; top: 15%; left: 5%; flex-direction: column; overflow: hidden; position: absolute;">
        <ui:VisualElement name="Header" style="height: 3%; flex-direction: row; align-items: center; border-left-width: 7px; border-right-width: 7px; border-top-width: 7px; border-bottom-width: 7px;">
            <ui:VisualElement style="width: 35px; height: 35px;" />
            <ui:VisualElement name="ItemCategoryTemplete" style="width: 80%;">
                <ui:VisualElement class="templete00-color-4 item-category" style="height: 20px; border-left-width: 5px; border-right-width: 5px; border-top-width: 5px; border-bottom-width: 5px; border-top-left-radius: 5px; border-top-right-radius: 5px; align-items: stretch; justify-content: space-between; background-color: rgba(0, 168, 232, 0.42);">
                    <ui:Label text="NAME" display-tooltip-when-elided="true" name="Text" style="width: 100%; height: 100%; left: 0; top: 0; right: 0; bottom: -19px; opacity: 1; padding-top: 0;" />
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="ItemCategoryTemplete" style="width: 20%; margin-left: 5px;">
                <ui:VisualElement class="templete00-color-4 item-category" style="height: 20px; border-left-width: 5px; border-right-width: 5px; border-top-width: 5px; border-bottom-width: 5px; border-top-left-radius: 5px; border-top-right-radius: 5px; align-items: stretch; justify-content: space-between; background-color: rgba(0, 168, 232, 0.42);">
                    <ui:Label text="COUNT" display-tooltip-when-elided="true" name="Text" style="width: 100%; height: 100%; left: 0; top: 0; right: 0; bottom: -19px; opacity: 1; padding-top: 0;" />
                </ui:VisualElement>
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:ScrollView style="height: 100%;">
            <ui:VisualElement name="ItemContainer" style="width: 100%; height: 100%;" />
        </ui:ScrollView>
    </ui:VisualElement>
    <ui:VisualElement name="LoadoutView" class="default-border" style="left: 1534px; width: 15%; height: 80%; top: 110px; position: absolute;">
        <ui:VisualElement name="Body" style="width: 100%; height: 70%;">
            <ui:VisualElement name="Head" style="background-color: rgba(0, 0, 0, 0.54); width: 100%; height: 20%;" />
            <ui:VisualElement name="Torso" style="background-color: rgba(0, 0, 0, 0.54); width: 100%; height: 40%; flex-direction: row;">
                <ui:VisualElement name="LeftArm" style="width: 30%; height: 100%;" />
                <ui:VisualElement style="height: 100%; width: 5%;" />
                <ui:VisualElement name="Chest" style="width: 30%; height: 100%;" />
                <ui:VisualElement style="height: 100%; width: 5%;" />
                <ui:VisualElement name="Chest" style="width: 30%; height: 100%;" />
            </ui:VisualElement>
            <ui:VisualElement name="Legs" style="background-color: rgba(0, 0, 0, 0.54); width: 100%; height: 40%; flex-direction: row;">
                <ui:VisualElement name="LeftLeg" style="width: 50%; height: 100%;" />
                <ui:VisualElement name="RightLeg" style="width: 50%; height: 100%;" />
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>

.mainmenu-button {
    height: 35px;
    width: 289px;
    align-items: stretch;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    -unity-font-definition: resource('Fonts & Materials/LiberationSans SDF');
    font-size: 15px;
    background-color: rgb(0, 0, 0);
}

.mainmenu-button:hover {
    height: 35px;
    width: 289px;
    align-items: stretch;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    -unity-font-definition: resource('Fonts & Materials/LiberationSans SDF');
    font-size: 15px;
    background-color: rgb(87, 87, 87);
}

.mainmenu-button:active {
    height: 35px;
    width: 289px;
    align-items: stretch;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    -unity-font-definition: resource('Fonts & Materials/LiberationSans SDF');
    font-size: 15px;
    background-color: rgb(188, 188, 188);
    color: rgb(60, 60, 60);
}

.default-border {
    border-left-width: 7px;
    border-right-width: 7px;
    border-top-width: 7px;
    border-bottom-width: 7px;
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    padding-top: 0;
}

.item {
    align-items: center;
    justify-content: flex-start;
    flex-direction: row;
    border-left-width: 5px;
    border-right-width: 5px;
    border-top-width: 5px;
    border-bottom-width: 5px;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    background-color: rgba(255, 255, 255, 0);
    height: 35px;
    color: rgb(255, 255, 255);
    margin-top: 3px;
    overflow: hidden;
}

.item:hover {
    align-items: center;
    justify-content: flex-start;
    flex-direction: row;
    border-left-width: 5px;
    border-right-width: 5px;
    border-top-width: 5px;
    border-bottom-width: 5px;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    background-color: rgba(255, 255, 255, 0.43);
    height: 35px;
    margin-top: 3px;
    overflow: hidden;
}

.item-category {
    height: 20px;
    border-left-width: 5px;
    border-right-width: 5px;
    border-top-width: 5px;
    border-bottom-width: 5px;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    align-items: stretch;
    justify-content: space-between;
    background-color: rgba(0, 168, 232, 0.42);
    -unity-text-align: middle-left;
    color: rgb(255, 255, 255);
    -unity-font: url('/Assets/CoreSystem/Runtime/Presentation/DefaultAssets/Resources/Fonts/Electronic Highway Sign.TTF');
    -unity-font-definition: resource('Fonts/UI/Electronic Highway Sign SDF');
}

.item-container.header {
    color: rgb(255, 255, 255);
    font-size: 20px;
    -unity-text-align: middle-left;
    padding-top: 0;
    -unity-font: resource('Fonts/Rajdhani-Regular');
    -unity-font-definition: resource('Fonts/UI/Rajdhani-Regular SDF');
}

.item-container.quantity {
    color: rgb(255, 255, 255);
    font-size: 15px;
    padding-top: 0;
    -unity-text-align: middle-left;
    -unity-font: resource('Fonts/Electronic Highway Sign');
    -unity-font-definition: resource('Fonts/UI/Electronic Highway Sign SDF');
}

.item-header {
    -unity-text-align: middle-left;
    padding-top: 0;
    font-size: 20px;
    -unity-font: resource('Fonts/Rajdhani-Regular');
    -unity-font-definition: resource('Fonts/UI/Rajdhani-Regular SDF');
}

.item-quantity {
    font-size: 15px;
    -unity-text-align: middle-left;
    color: rgb(255, 255, 255);
    padding-top: 0;
    -unity-font: url('/Assets/CoreSystem/Runtime/Presentation/DefaultAssets/Resources/Fonts/Electronic Highway Sign.TTF');
    -unity-font-definition: resource('Fonts/UI/Electronic Highway Sign SDF');
}

.loadout-item {
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
}
